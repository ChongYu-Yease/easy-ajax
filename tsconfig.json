{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    "target": "es5",
    "lib": [
      "ES2016",
      "DOM"
    ],
    "jsx": "preserve",
    "module": "ES2015",
    // "rootDir": "./",
    "moduleResolution": "node",
    "declaration": true,
    "downlevelIteration": true,
    "declarationDir": "./temp",
   "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}